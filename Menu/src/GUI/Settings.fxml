<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/9.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.SettingsController">
    <JFXButton buttonType="RAISED" layoutX="328.0" layoutY="265.0" onAction="#applySettings" prefHeight="14.0"
               prefWidth="58.0" style="-fx-background-color: ffffff;" text="Apply"/>
    <JFXButton buttonType="RAISED" layoutX="226.0" layoutY="265.0" onAction="#backToMenu" prefHeight="18.0"
               prefWidth="90.0" style="-fx-background-color: ffffff;" text="Back to menu"/>
    <JFXTextField fx:id="textNickName" labelFloat="true" layoutX="14.0" layoutY="14.0" prefHeight="25.0"
                  prefWidth="189.0" promptText="NickName:" unFocusColor="#00ff40"/>
    <JFXTextField fx:id="textServerIP" labelFloat="true" layoutX="14.0" layoutY="52.0" prefHeight="25.0"
                  prefWidth="189.0" promptText="Server IP:" unFocusColor="#00ff40"/>
    <JFXTextField fx:id="textServerPort" labelFloat="true" layoutX="14.0" layoutY="92.0" prefHeight="25.0"
                  prefWidth="189.0" promptText="Server PORT:" unFocusColor="#00ff40"/>
    <JFXTextField fx:id="textWindowWidth" labelFloat="true" layoutX="14.0" layoutY="135.0" prefHeight="25.0"
                  prefWidth="189.0" promptText="Window width:" unFocusColor="#00ff40"/>
    <JFXTextField fx:id="textWindowHeight" labelFloat="true" layoutX="14.0" layoutY="174.0" prefHeight="25.0"
                  prefWidth="189.0" promptText="Window height:" unFocusColor="#00ff40"/>
    <JFXTextField fx:id="textFPS" labelFloat="true" layoutX="14.0" layoutY="217.0" prefHeight="25.0" prefWidth="189.0"
                  promptText="FPS:" unFocusColor="#00ff40"/>
    <ImageView fx:id="imageViewCharacter" fitHeight="91.0" fitWidth="90.0" layoutX="271.0" layoutY="90.0"
               onMouseClicked="#nextCharacter" pickOnBounds="true" preserveRatio="true"/>
</AnchorPane>
